{
  "family": "patient-survival-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "survival-app-container",
      "image": "${IMAGE_URI}:latest",
      "essential": true,
      "memory": 4096,
      "cpu": 1024,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "4096"
}